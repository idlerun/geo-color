{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es6",
      "dom.iterable",
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": false,
    "noEmit": true,
    "jsx": "preserve",
    "incremental": true,
    "noImplicitAny": false
  },
  "include": [
    "src",
    "pages",
    "data"
  ],
  "exclude": [
    "node_modules"
  ]
}
